{
  "name": "oxid-esales/oxideshop-doctrine-migration-wrapper",
  "description": "OXID eShop database migrations using doctrine",
  "license": "GPL-3.0",
  "require": {
    "doctrine/migrations": "^1.4.1",
    "oxid-esales/oxideshop-facts": "^2.0.0",
    "symfony/console": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "^4.8.26",
    "mikey179/vfsStream": "~1.4.0"
  },
  "autoload": {
    "psr-4": {
      "OxidEsales\\DoctrineMigrations\\": "./src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "OxidEsales\\DoctrineMigrations\\Tests\\": "./tests"
    }
  },
  "bin": [
    "./bin/oe-eshop-db_migrate",
    "./bin/oe-eshop-doctrine_migration"
  ]
}
