{
  "name": "oxid-esales/oxideshop-doctrine-migration-wrapper",
  "description": "OXID eShop database migrations using doctrine",
  "license": "GPL-3.0-only",
  "require": {
    "php": "^7.4 || ^8.0",
    "doctrine/migrations": "^2.3.2",
    "composer/package-versions-deprecated": "^1",
    "oxid-esales/oxideshop-facts": "^2.4.0",
    "symfony/console": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "^8.5.14",
    "mikey179/vfsstream": "~1.6.8",
    "symfony/filesystem": "^4.4.17",
    "symfony/yaml": "~3.4 || ~4.0",
    "oxid-esales/oxideshop-ce": "^6.9.0",
    "ext-pdo": "*"
  },
  "autoload": {
    "psr-4": {
      "OxidEsales\\DoctrineMigrationWrapper\\": "./src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "OxidEsales\\DoctrineMigrationWrapper\\Tests\\": "./tests"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "bin": [
    "./bin/oe-eshop-db_migrate",
    "./bin/oe-eshop-doctrine_migration"
  ]
}
