{
  "name": "oxid-esales/oxideshop-doctrine-migration-wrapper",
  "description": "OXID eShop database migrations using doctrine",
  "license": "GPL-3.0-only",
  "require": {
    "php": "^7.4 || ^8.0",
    "doctrine/migrations": "^2.3.2",
    "composer/package-versions-deprecated": "^1",
    "oxid-esales/oxideshop-facts": "^3.0.0",
    "symfony/console": "^5.0.4"
  },
  "require-dev": {
    "phpunit/phpunit": "^9",
    "phpspec/prophecy-phpunit": "^v2.0.1",
    "mikey179/vfsstream": "^1.6",
    "symfony/filesystem": "^5.0.4",
    "symfony/yaml": "^v5.0.5",
    "oxid-esales/oxideshop-ce": "dev-master",
    "ext-pdo": "*"
  },
  "autoload": {
    "psr-4": {
      "OxidEsales\\DoctrineMigrationWrapper\\": "./src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "OxidEsales\\DoctrineMigrationWrapper\\Tests\\": "./tests"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "bin": [
    "./bin/oe-eshop-db_migrate",
    "./bin/oe-eshop-doctrine_migration"
  ]
}
